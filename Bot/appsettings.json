{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Telegram": {
    "Token": "<Telegram bot token>"
  },
  "Services": {
    "CleanerQueueUrl": "<Queue name for cleaner service>",
    "ConverterQueueUrl": "<Queue name for converter service>",
